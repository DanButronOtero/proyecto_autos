<%- include('../templates/header'); -%>
    <!-- Banner Area -->
    <div id="banner" class="banner style-2">
        <div class="banner-item banner-1 steller-parallax" data-stellar-background-ratio="0.5">
            <div class="banner-content">
                <div class="container justify-content-md-center">
                    <div class="row ">
                        <div class="col-xs-12">
                            <div class="banner-text-content text-center wow fadeInUp" data-wow-duration="1.5s">
                                <h1 class="banner-title ">Coches </h1>
                            </div>
                        </div>
                        <div class="col-xs-12">
                            <div class="banner-text-content text-center wow fadeInUp" data-wow-duration="1.5s">
                                <table class="table">
                                    <thead class="thead-light">
                                        <th>id</th>
                                        <th>matricula</th>
                                        <th>numero de motor</th>
                                        <th>marca</th>
                                        <th>modelo</th>
                                        <th>a√±o</th>
                                        <th>editar</th>
                                        <th>borrar</th>
                                    </thead>
                                    <% for (let i = 0; i < registros.length; i++) { %>
                                        <thead>
                                            <th>
                                                <%= registros[i].id %>
                                            </th>
                                            <th>
                                                <%= registros[i].matricula %>
                                            </th>
                                            <th>
                                                <%= registros[i].no_motor %>
                                            </th>
                                            <th>
                                                <%= registros[i].marca%>
                                            </th>
                                            <th>
                                                <%= registros[i].modelo%>
                                            </th>
                                            <th>
                                                <%= registros[i].anio%>
                                            </th>
                                            <th>
                                                <form action="/coches/edit" method="post">
                                                    <button type="submit" class="btn btn-primary" aria-label="Left Align">
                                                        <input type="hidden" name="id" value="<%=registros[i].id  %> ">
                                                        <span class="glyphicon glyphicon-pencil" aria-hidden="true"></span>
                                                      </button>
                                                </form>

                                            </th>
                                            <th>
                                                <form action="/coches/delete" method="post">
                                                    <button type="submit" class="btn btn-primary" aria-label="Left Align">
                                                        <input type="hidden" name="id" value="<%=registros[i].id  %> ">
                                                        <span class="glyphicon glyphicon-trash" aria-hidden="true"></span>
                                                      </button>
                                                </form>
                                            </th>

                                        </thead>

                                        <% } %>
                                </table>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- Banner Area End -->
    <%- include('../templates/footer'); -%>